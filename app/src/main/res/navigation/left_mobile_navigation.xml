<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.android.step.client.ui.home.HomeFragment"
        android:label="左侧"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.android.step.client.ui.visualization.VisualizationFragment"
        android:label="左侧"
        tools:layout="@layout/fragment_visualization" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.android.step.client.ui.config.ConfigFragment"
        android:label="左侧"
        tools:layout="@layout/fragment_config" />
</navigation>